{
  "name": "olnode",
  "version": "1.7.4",
  "description": "OVERLINE EMB & OL MINER",
  "main": "lib/src/init.js",
  "scripts": {
    "bcnode": "./bin/cli start --ws --rovers --ui --node --miner-key YOUR_PUBLIC_KEY",
    "benchmark": "./bin/benchmark",
    "check": "npm run dist",
    "compile-parser": "./node_modules/.bin/jison src/script/script.jison -o src/script/script.js && node ./scripts/parse-add-eslint-ignore.js",
    "doc": "./node_modules/.bin/esdoc",
    "docker:build": "npm run dist && docker build -t blockcollider/bcnode .",
    "fixme": "./node_modules/fixme/bin/cli src/**/*.es6",
    "flow:update": "npm run rimraf flow-typed && npm run flow-typed update",
    "lint:eslint": "true || npm run eslint src/**/*.es6",
    "lint:eslint:watch": "onchange 'src/**/*.es6' -i -- npm lint:eslint",
    "lint:flow": "true",
    "version:generate": "node ./scripts/version-generate.js",
    "transpile": "babel src --source-maps=true --config-file ./.babelrc --out-dir lib --copy-files --ignore ./**/__tests__/*,./lib/,./node_modules,./.babelrc,./package.json,./npm-debug.log",
    "test": "jest",
    "build": "npm run lint:flow && npm run lint:eslint && npm run transpile --copy-files && npm run version:generate && npm run webpack",
    "build-windows": "npm run lint:eslint && npm run transpile --copy-files && npm run version:generate && npm run webpack",
    "dist": "npm && npm run build && npm test --ci --coverage && npm audit || true && npm run outdated && npm run doc",
    "dist-windows": "npm && npm run build-native-windows && npm test --ci --coverage && npm audit || true && npm run outdated && npm run doc",
    "outdated": "npm outdated || true",
    "pkg": "pkg package.json --out-path=dist",
    "watch:lint": "onchange src/**/*.es6 -- npm run lint:eslint",
    "watch:transpile": "onchange src/**/*.es6 -- babel {{changed}} --source-maps=true --config-file ./.babelrc --out-dir lib --ignore **/__tests__/*,lib,node_modules",
    "watch:test": "jest --watch",
    "watch": "concurrently -p name -n 'standard,babel,jest' 'npm run watch:lint' 'npm run watch:transpile' 'npm run watch:test'",
    "webpack": "./node_modules/webpack-cli/bin/cli.js --config webpack/webpack.config.js"
  },
  "engines": {
    "node": "12.x"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/blockcollider/bcnode.git"
  },
  "author": {
    "name": "Overline Network",
    "email": "no-reply@github.com",
    "url": "https://docs.overline.network/"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/blockcollider/bcnode/issues"
  },
  "homepage": "https://github.com/blockcollider/bcnode#readme",
  "bin": {
    "cli": "lib/cli/main.js"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "jsx",
      "json",
      "ts",
      "tsx",
      "es6",
      "node"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/*.*",
      "node_modules/jest/node_modules/jest-cli/build/test_scheduler.js",
      "/lib/"
    ],
    "testEnvironment": "node",
    "testMatch": [
      "**/__tests__/**/*.(js|es6)"
    ],
    "transform": {
      "^.+\\.(js|jsx|es6)$": "babel-jest"
    },
    "collectCoverage": false
  },
  "standard": {
    "globals": [
      "describe",
      "context",
      "beforeAll",
      "beforeEach",
      "afterAll",
      "afterEach",
      "it",
      "test",
      "expect",
      "jest",
      "location",
      "TimeoutID",
      "IntervalID",
      "$Values",
      "$Keys"
    ],
    "parser": "babel-eslint",
    "plugins": []
  },
